{
  "name": "npm-publish-ts",
  "version": "0.0.0",
  "description": "npm-publish-ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "del": "ts-node ./scripts/del",
    "lint": "cross-env NODE_ENV=production eslint --ext .js,.ts ./",
    "fix": "cross-env NODE_ENV=production eslint --fix --ext .js,.ts ./",
    "ttsc": "ttsc --p ./tsconfig.json",
    "test": "jest",
    "dev": "npm run del && npm run ttsc -- --watch",
    "build": "npm run del && npm run ttsc && npm run test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhouhuafei/npm-publish-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhouhuafei/npm-publish-ts/issues"
  },
  "homepage": "https://github.com/zhouhuafei/npm-publish-ts#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2",
    "typescript-transform-paths": "^3.3.1"
  }
}
